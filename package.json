{
	"name": "scq",
	"version": "0.0.1",
	"description": "Everything you need to build a Svelte library, powered by [`sv`](https://npmjs.com/package/sv).",
	"keywords": [
		"svelte"
	],
	"homepage": "https://github.com/techasit5415/SCQ#readme",
	"bugs": {
		"url": "https://github.com/techasit5415/SCQ/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/techasit5415/SCQ.git"
	},
	"license": "ISC",
	"author": "",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"main": "svelte.config.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"scripts": {
		"dev": "vite --host 0.0.0.0 --port 5173",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
		"bcrypt": "^6.0.0",
		"chart.js": "^4.5.0",
		"it": "^1.1.1",
		"mariadb": "^3.4.4",
		"mysql2": "^3.14.2",
		"pocketbase": "^0.26.2",
		"svelte-sonner": "^1.0.5"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js"
}
